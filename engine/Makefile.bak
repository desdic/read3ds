## Compiler
CC=g++
## Linker
LD=$(CC)
## Flags
CPPDEBUG=-g -Wall -w -Wextra -pedantic
# -pedantic  -pedantic-errors -w -Wextra
#OPTIMIZE=-march=pentium4 -msse3
CPPFLAGS = -O3 $(OPTIMIZE) $(CPPDEBUG) -DLINUX `sdl-config --cflags`
LFLAGS = `sdl-config --libs` -lGL -lGLU -lSDL_image -lGLEW

DEPEND=makedepend

HEADERFILES = main.h ivector.h light.h material.h read3ds.h camera.h object.h iface.h mapping.h scene.h shader.h
OBJS = main.o drawing.o sjov.o texture.o read3ds.o material.o camera.o object.o scene.o shader.o
SRCS = main.cc drawing.cc sjov.cc texture.cc read3ds.cc material.cc camera.cc object.cc scene.cc shader.cc
APP = sjov
#TESTSRC = test.cc read3ds.cc texture.cc
#TESTOBJ = test.o read3ds.o texture.o
#TEST = test

#CHUNK = chunk
#CHUNKOBJ = chunk.o object.o material.o camera.o texture.o scene.o
#CHUNKSRC = chunk.cc object.cc material.cc camera.cc texture.cc scene.cc

all : $(APP) $(TEST) $(CHUNK)

#$(CHUNK): $(CHUNKOBJ)
#	$(LD) -o $@ ${CHUNKOBJ} ${LFLAGS}

clean :
	rm -rf $(APP) $(TEST) *.o core

$(APP): ${OBJS}
	$(LD) -o $@ ${OBJS} ${LFLAGS}

#$(TEST): ${TESTOBJ}
#	$(LD) -o $@ ${TESTOBJ} ${LFLAGS}

depend :
	$(DEPEND) ${CPPFLAGS} ${SRCS}

